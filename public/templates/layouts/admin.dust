    <div class="container-fluid">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="/">Apps for PayPal Here <b>ADMIN</b></a>
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                <li {@eq key=active value="users"}class="active"{/eq}><a href="/admin/users">Users</a></li>
                <li {@eq key=active value="logs"}class="active"{/eq}><a href="/admin/logs">Logs</a></li>
            </ul>

            <ul class="nav navbar-nav navbar-right">
                <li><a href="/settings">{user.email}</a></li>
                <li><a href="/oauth/logout">Log Out</a></li>
            </ul>

        </div><!--/.nav-collapse -->
    </div>
</div>

    {+body /}

    {+bodyElements /}

<script src="/components/jquery/dist/jquery.min.js"></script>
<script src="/components/bootstrap/dist/js/bootstrap.min.js"></script>
<script src="/components/fuelux/dist/js/fuelux.js" type="text/javascript"></script>
<script src="/js/jquery-dateFormat.min.js" type="text/javascript"></script>
<script src="/components/accounting/accounting.min.js" type="text/javascript"></script>
<script src="/components/underscore/underscore.js" type="text/javascript"></script>
